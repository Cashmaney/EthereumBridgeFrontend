.light{
  background: linear-gradient(180deg, #FFFFFF 0%, #F8EC99 180.99%);
  width:100%;
  min-height: calc(100vh - 116px);
  padding-top:42px;
}

.dark{
  background: linear-gradient(180deg, #363636 0%, #000000 180.99%);
  width:100%;
  min-height: calc(100vh - 116px);
  padding-top:42px;
  scrollbar-color: dark;
}
